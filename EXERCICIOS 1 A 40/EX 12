#include<iostream> 
int main(){ 
int num1; 
int num2; 
int multiplicacao; 
std::cout <<"digite o numero:  " << std::endl; 
std::cin >> num1; 

std::cout <<"digite outro numero:   "<< std::endl; 
std::cin >> num2; 

multiplicacao = num1 * num2; 
std::cout <<"a multiplicacao eh : "<< multiplicacao << std::endl; 
return 0; 
}
