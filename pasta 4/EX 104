//Armazenar 10 nomes em um vetor e exibir uma lista numerada
#include <iostream> 
#include <string>
using namespace std;
int main()
{
    string nomes[10];
    
    for(int i=0; i<10;  i++) { 
    cout<<"escreva um nome:  "; 
    cin >> nomes[i]; 
    } 
    
    for(int i=0; i<10; i++) { 
    cout<<i+1<<nomes[i]<< endl;
    }
    return 0;
}
