
#include <iostream>
using namespace std;
int main(){
int num1; 

    cout<<"digite um numero: "; 
    cin >> num1; 
   
    if(num1 % 3 == 0 ) { 
    std::cout <<"eh divisivel por 3"; 
    }else if(num1 % 7 == 0 ){ 
    std::cout <<"eh divisivel por 7";
    } else { 
        cout<<"nao eh divisivel nem por 3 nem por 7";
    
    
    return 0;
    
}   
}
